pipeline {
    agent any

    stages {
	 stage('Print') {
            steps {
                echo 'in work..'
            }
			}
		stage("cloning"){
			steps{
				script {
					git credentialsId: 'userIdForGit', url: 'https://github.com/rahulrv2910/DemoSpringBootApplication.git'
              
                    }
			}
			}
		stage("building project"){
			steps{
				script{
					sh 'mvn clean install'
		
		}
		}
		}
       
        
        
        }
    }
}